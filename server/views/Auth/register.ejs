<%- include('../partials/headercode.ejs') %>

    <body>

        <!-- Content -->

        <div class="container-xxl">
            <div class="authentication-wrapper authentication-basic container-p-y">
                <div class="authentication-inner">

                    <!-- Register Card -->
                    <div class="card">
                        <div class="card-body">
                            <!-- Logo -->
                            <div class="app-brand justify-content-center">
                                <a href="" class="app-brand-link gap-2">
                                    <span class="app-brand-logo demo">

                                        

                                    </span>
                                    <span class="app-brand-text demo text-body fw-bolder">Dawa.ai</span>
                                </a>
                            </div>
                            <!-- /Logo -->
                            <h4 class="mb-2">Welcome to Dawa.ai! ðŸ‘‹</h4>
                            <p class="mb-4">Make your app management easy and fun!</p>

                            <form id="formAuthentication" class="mb-3"
                                action=""
                                method="POST">

                                <div class="mb-3">
                                    <label for="mobilenumber" class="form-label">Mobile No.</label>
                                    <input type="number" class="form-control" id="mobilenumber" name="mobilenumber"
                                        placeholder="Enter 10-digit mobile no.">
                                </div>

                                <div class="mb-3">
                                    <label for="ownername" class="form-label">Owner Name</label>
                                    <input type="text" class="form-control" id="ownername" name="ownername"
                                        placeholder="Enter owner name" autofocus="">
                                </div>
                                
                                <div class="mb-3">
                                    <label for="email" class="form-label">Email</label>
                                    <input type="email" class="form-control" id="email" name="email"
                                        placeholder="Enter your email address" autofocus="">
                                </div>

                                <div class="mb-3">
                                    <label for="gstin" class="form-label">GSTIN</label>
                                    <input type="gstin" class="form-control" id="gstin" name="gstin"
                                        placeholder="Enter GSTIN" autofocus="">
                                </div>


                               
                                <div class="mb-3">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="terms-conditions"
                                            name="terms">
                                        <label class="form-check-label" for="terms-conditions">
                                            I agree to
                                            <a href="javascript:void(0);">privacy policy &amp; terms</a>
                                        </label>
                                    </div>
                                </div>
                                <button class="btn btn-primary d-grid w-100">
                                    Get OTP
                                </button>
                            </form>

                            <p class="text-center">
                                <span>Already have an account?</span>
                                <a href="/login">
                                    <span>Sign in instead</span>
                                </a>
                            </p>
                        </div>
                    </div>
                    <!-- Register Card -->
                </div>
            </div>
        </div>

        <!-- / Content -->



        <%- include('../partials/footercode.ejs') %>