<%- include('../partials/headercode.ejs') %>

    <body>

        <!-- Layout wrapper -->
        <div class="layout-wrapper layout-content-navbar  ">
            <div class="layout-container">

                <!-- sidebar imported -->
                <%- include('../partials/sidebar.ejs') %>

                    <!-- Layout container -->
                    <div class="layout-page">

                        <!-- navbar imported -->
                        <%- include('../partials/navbar.ejs') %>

                            <!-- Content wrapper -->
                            <div class="content-wrapper">

                                <!-- Content -->

                                <div class="container-xxl flex-grow-1 container-p-y">

                                    <h4 class="fw-bold py-3 mb-4">
                                        <span class="fw-light">User Profile</span>
                                    </h4>

                                    <!-- Header -->
                                    <div class="row">
                                        <div class="col-12">
                                            <div class="card mb-2">
                                                <!-- <div class="user-profile-header-banner">
                                                    <img src="/img/backgrounds/profile-banner.png" alt="Banner image"
                                                        class="rounded-top w-100">
                                                </div> -->
                                                <div
                                                    class="user-profile-header d-flex flex-column flex-sm-row text-sm-start text-center mb-4">
                                                    <div class="flex-shrink-0 mt-n2 mx-sm-0 mx-auto">
                                                        <img src="/img/avatars/1.jpg" alt="user image"
                                                            class="d-block h-auto ms-0 ms-sm-4 rounded user-profile-img w-75">
                                                    </div>
                                                    <div class="flex-grow-1 mt-3 mt-sm-5">
                                                        <div
                                                            class="d-flex align-items-md-end align-items-sm-start align-items-center justify-content-md-between justify-content-start mx-4 flex-md-row flex-column gap-4">
                                                            <div class="user-profile-info">
                                                                <h4>
                                                                    <%=data[0].owner_name%>
                                                                </h4>
                                                                <ul
                                                                    class="list-inline mb-0 d-flex align-items-center flex-wrap justify-content-sm-start justify-content-center gap-2">
                                                                    <li class="list-inline-item fw-semibold">
                                                                        <i class='bx bx-user'></i> Pharmacy Owner
                                                                    </li>
                                                                    <li class="list-inline-item fw-semibold">
                                                                        <i class='bx bx-map'></i>
                                                                        <%=data[0].org_city%>
                                                                    </li>
                                                                    <li class="list-inline-item fw-bold" >
                                                                        <i class='bx bx-calendar-alt'></i><span id="joinedDate"></span>
                                                                    </li>
                                                                    
                                                                </ul>
                                                            </div>
                                                            <a href="/update_profile"
                                                                class="btn btn-primary text-nowrap">
                                                                <i class="bx bx-user-check me-1"></i>Update Profile
                                                            </a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/ Header -->

                                    <!-- User Profile Content -->
                                    <div class="row d-flex justify-content-around mt-3">
                                        <div class="col-xl-6 col-lg-6 col-md-6  ">
                                            <!-- About User -->
                                            <div class="card mb-4">
                                                <div class="card-body">
                                                    <small class="text-muted text-uppercase">About</small>
                                                    <ul class="list-unstyled mb-4 mt-3">
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Pharmacy Name:</span> <span>
                                                                <%=data[0].org_name%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Owner Name:</span> <span>
                                                                <%=data[0].owner_name%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">GSTIN:</span> <span>
                                                                <%=data[0].org_gstin%>
                                                            </span></li>
                                                    </ul>
                                                    <small class="text-muted text-uppercase">License</small>
                                                    <ul class="list-unstyled mb-4 mt-3">
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">DL No 1:</span> <span>
                                                                <%=data[0].org_dl_no_1%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">DL No 2:</span> <span>
                                                                <%=data[0].org_dl_no_2%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">DL No 3:</span> <span>
                                                                <%=data[0].org_dl_no_3%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">DL No 4:</span> <span>
                                                                <%=data[0].org_dl_no_4%>
                                                            </span></li>

                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">FSSAI No:</span> <span>
                                                                <%=data[0].org_fssai_no%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Shop License:</span> <span>
                                                                <%=data[0].org_shop_lic%>
                                                            </span></li>
                                                    </ul>
                                                </div>
                                            </div>
                                            <!--/ About User -->
                                        </div>
                                        <div class="col-xl-6 col-lg-6 col-md-6  ">
                                            <!-- About User -->
                                            <div class="card mb-4">
                                                <div class="card-body">
                                                    <small class="text-muted text-uppercase">Location</small>
                                                    <ul class="list-unstyled mb-4 mt-3">
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">State:</span> <span>
                                                                <%=data[0].org_state%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">City:</span> <span>
                                                                <%=data[0].org_city%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Address:</span> <span>
                                                                <%=data[0].org_address%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Pincode:</span> <span>
                                                                <%=data[0].org_pincode%>
                                                            </span></li>

                                                    </ul>
                                                    <small class="text-muted text-uppercase">Contacts</small>
                                                    <ul class="list-unstyled mb-4 mt-3">
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Contact:</span> <span>
                                                                <%=data[0].org_telephone%>
                                                            </span></li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Alternate Contact:</span>
                                                            <span>
                                                                <%=data[0].org_alt_telephone%>
                                                            </span>
                                                        </li>
                                                        <li class="d-flex align-items-center mb-3"><span
                                                                class="fw-semibold mx-2">Email:</span> <span>
                                                                <%=data[0].org_email%>
                                                            </span></li>
                                                    </ul>
                                                </div>
                                            </div>
                                            <!--/ About User -->
                                        </div>
                                    </div>
                                    <!-- / User Profile Content -->
                                </div>
                                <!-- / Content -->

                                <div class="content-backdrop fade"></div>
                            </div>
                            <!-- Content wrapper -->
                    </div>
                    <!-- / Layout page -->
            </div>



            <!-- Overlay -->
            <div class="layout-overlay layout-menu-toggle"></div>


        </div>
        <!-- / Layout wrapper -->

        <%- include('../partials/footercode.ejs') %>

            <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
            <script>
                $(document).ready(function () {

                    var datetimeString = "<%=data[0].org_created%>"
                    var datetime = moment(datetimeString, "ddd MMM DD YYYY HH:mm:ss [GMT]ZZ");

                    // Format the datetime in "Joined April 2021" format
                    var formattedDate = "Joined " + datetime.format("MMMM YYYY");

                    $("#joinedDate").text(formattedDate);
                })

            </script>

    </body>


    </html>